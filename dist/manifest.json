{
  "manifest_version": 3,
  "name": "AutoFACT",
  "version": "0.1",
  "description": "Automated Fact and Content Tester",
  "action": {
    "default_popup": "icon.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["my_styles.css"],
      "js": ["content_script.js"]
    }
  ]
}